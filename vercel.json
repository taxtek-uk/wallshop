{
  "functions": {
    "api/**/*": {
      "runtime": "nodejs20.x"
    }
  },
  "buildCommand": "npx vite build",
  "outputDirectory": "dist/spa",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!api).+)",
      "destination": "/index.html"
    }
  ]
}